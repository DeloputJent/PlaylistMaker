<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="YP_Black">#1A1B22</color>
    <color name="YP_Blue">#3772E7</color>
    <color name="YP_Blue_Light">#9FBBF3</color>
    <color name="YP_Dark_Gray">#404040</color>
    <color name="YP_Gray">#AEAFB4</color>
    <color name="YP_Light_Gray">#E6E8EB</color>
    <color name="YP_White">#FFFFFFFF</color>
    <color name="switchthumb_color">#3772E7</color>
    <color name="switchtrack_color">#9FBBF3</color>
    <style name="Base.Theme.PlaylistMaker" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="colorPrimary">@color/YP_Black</item>
        <item name="colorOnPrimary">@color/YP_White</item>
        <item name="colorSecondary">@color/YP_Black</item>
        <item name="colorOnSecondary">@color/YP_White</item>
        
        
    </style>
    <style name="Black_White_Text13sp_regular">
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textColor">@color/YP_White</item>
        <item name="android:textSize">13sp</item>
        <item name="android:fontWeight">400</item>
        <item name="android:lineHeight">100dp</item>
        <item name="android:letterSpacing">0</item>
    </style>
    <style name="Black_White_Text14sp_medium">
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textColor">@color/YP_White</item>
        <item name="android:textSize">14sp</item>
        <item name="android:fontWeight">400</item>
        <item name="android:lineHeight">100dp</item>
        <item name="android:letterSpacing">0</item>
    </style>
    <style name="Black_White_Text22sp_medium">
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textColor">@color/YP_White</item>
        <item name="android:textSize">22sp</item>
        <item name="android:fontWeight">400</item>
        <item name="android:lineHeight">100dp</item>
        <item name="android:letterSpacing">0</item>
    </style>
    <style name="ButtonOnMainStyle" parent="Widget.MaterialComponents.Button.UnelevatedButton">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_marginStart">@dimen/margin_medium</item>
        <item name="android:layout_marginTop">@dimen/margin_medium</item>
        <item name="android:layout_marginEnd">@dimen/margin_medium</item>
        <item name="android:layout_weight">333</item>
        <item name="android:backgroundTint">#ffffff</item>
        <item name="android:font">@font/ys_display_medium</item>
        <item name="android:letterSpacing">0</item>
        <item name="android:textSize">22sp</item>
        <item name="android:textColor">@color/YP_Black</item>
        <item name="android:textAllCaps">false</item>
    </style>
    <style name="Gray_White_Text13sp_regular">
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textColor">@color/YP_White</item>
        <item name="android:textSize">13sp</item>
        <item name="android:fontWeight">400</item>
        <item name="android:lineHeight">100dp</item>
        <item name="android:letterSpacing">0</item>
    </style>
    <style name="GreyText11sp">
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textColor">@color/YP_White</item>
        <item name="android:textSize">11sp</item>
        <item name="android:fontWeight">400</item>
        <item name="android:lineHeight">100dp</item>
        <item name="android:letterSpacing">0</item>
    </style>
    <style name="GreyText16sp">
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textColor">@color/YP_Black</item>
        <item name="android:textColorHint">@color/YP_Black</item>
        <item name="android:textSize">16sp</item>
        <item name="android:fontWeight">400</item>
        <item name="android:lineHeight">100dp</item>
        <item name="android:letterSpacing">0</item>
    </style>
    <style name="InvisibleButton56x40OnSettings">
        <item name="android:layout_width">56dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:layout_marginEnd">6dp</item>
        <item name="android:layout_gravity">end|center_vertical</item>
        <item name="android:backgroundTint">?attr/colorSecondary</item>
    </style>
    <style name="TextView16sp">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textColor">@color/YP_White</item>
        <item name="android:textSize">16sp</item>
        <item name="android:letterSpacing">0</item>
    </style>
</resources>